<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.google.samples.apps.iosched">

    <uses-permission
        android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"
        tools:node="remove"/>

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"
        tools:node="remove"/>

    <uses-permission
        android:name="android.permission.BLUETOOTH"
        tools:node="remove"/>
    <uses-permission
        android:name="android.permission.BLUETOOTH_ADMIN"
        tools:node="remove"/>
    <uses-feature
	    android:name="android.hardware.bluetooth_le"
	    android:required="false"
        tools:node="remove"/>

    <application>

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            tools:node="remove"/>

        <activity
            android:name=".ui.SessionLivestreamActivity"
            tools:node="remove"/>

        <activity
            android:name=".ui.NfcBadgeActivity"
            tools:node="remove"/>

        <activity
            android:name=".ui.VideoLibraryActivity"
            tools:node="remove"/>

        <activity
            android:name=".ui.NearbyEulaActivity"
            tools:node="remove" />

        <activity
            android:name=".ui.phone.MapActivity"
            tools:node="remove"/>

        <activity
            android:name=".ui.tablet.MapMultiPaneActivity"
            tools:node="remove"/>

        <activity
            android:name=".ui.ExpertsDirectoryActivity"
            tools:node="remove"/>

        <activity
            android:name=".ui.PeopleIveMetActivity"
            tools:node="remove"/>

    </application>
</manifest>
